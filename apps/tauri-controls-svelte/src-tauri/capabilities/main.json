{
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-maximize",
    "window:allow-start-dragging",
    "window:allow-toggle-maximize",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:default",
    "os:default"
  ]
}